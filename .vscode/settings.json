{
    "files.associations": {
        "app.css": "tailwindcss"
    },
    "tailwindCSS.experimental.classRegex": [
        [
            "name=\"([^\"]+)\""
        ],
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true,
        "**/site": false,
        "**/static": false,
    },
    "tailwindCSS.classAttributes": [
        "class",
        "className",
        "ngClass",
        "class:list",
        "name",
    ],
}